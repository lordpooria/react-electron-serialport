{
  "env": {
    "development": {
      "application/javascript": {
        "presets": ["es2016-node5", "react"],
        "plugins": [
          "transform-class-properties",
          "transform-async-to-generator",
          "transform-runtime",
          "transform-object-rest-spread",
          "transform-remove-console"
        ],
        "sourceMaps": "inline"
      },
      "text/less": {
        "dumpLineNumbers": "comments"
      }
    },
    "production": {
      "application/javascript": {
        "presets": ["es2016-node5", "react"],
        "plugins": [
          "transform-class-properties",
          "transform-async-to-generator",
          "transform-runtime",
          "transform-object-rest-spread",
          "transform-remove-console"
        ],
        "sourceMaps": "none"
      }
    }
  }
}